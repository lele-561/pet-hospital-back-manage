{
  "routes": [
    {
      "route": "/api/*",
      "allowedOrigins": [
        "https://pet-hospital.azurewebsites.net/{path}"
      ],
      "allowedMethods": [
        "GET",
        "POST"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  }
}

